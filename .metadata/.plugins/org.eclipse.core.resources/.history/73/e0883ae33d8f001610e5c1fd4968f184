#include <iostream>

#include "calculatorFunction.h";

using namespace std;

int main(int argc, const char * argv[]) {
	int a, b = {0};

	puts("Simple calculator!");

	puts("Choose what you would like!");
	puts("1: For add!");

	puts("Enter a");
	cin >> a;
	puts("Enter b");
	cin >> b;
	int sub = subtract(a, b);

	printf("Result is: %d", sub);


	return 0;

}



